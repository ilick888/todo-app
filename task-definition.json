{
  "family": "nginx",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "todo-app",
      "image": "todo-app",
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}
